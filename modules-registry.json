      "new_features": "Increment MINOR version",
      "bug_fixes": "Increment PATCH version"
    },
    "maturity_levels": {
      "alpha": "Early development, expect breaking changes",
      "beta": "Feature complete, testing in progress",
      "stable": "Production ready, backward compatible"
    },
    "compliance_tags": {
      "soc2": "SOC 2 Type II compliant",
      "pci-dss": "PCI DSS compliant",
      "hipaa": "HIPAA compliant"
    },
    "testing_requirements": {
      "alpha": "Basic validation tests",
      "beta": "Integration tests required",
      "stable": "Full test coverage including security scans"
    }

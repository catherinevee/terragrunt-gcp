{
  "name": "test-instance",
  "machine_type": "e2-micro",
  "image": "ubuntu-os-cloud/ubuntu-2004-lts",
  "disk_size": 20,
  "disk_type": "pd-standard",
  "zone": "europe-west1-a",
  "tags": ["test", "unit-test", "compute"],
  "labels": {
    "environment": "test",
    "team": "platform",
    "project": "terraform-gcp"
  },
  "network": "default",
  "subnetwork": "default",
  "service_account_email": "test-sa@project.iam.gserviceaccount.com",
  "service_account_scopes": [
    "https://www.googleapis.com/auth/cloud-platform",
    "https://www.googleapis.com/auth/compute"
  ],
  "startup_script": "#!/bin/bash\necho 'Starting test instance'\napt-get update\napt-get install -y nginx\nsystemctl start nginx\nsystemctl enable nginx\necho 'Test instance ready' > /var/www/html/index.html"
}
